{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go App",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "cmd/main.go",
      "args": ["${workspaceFolder}/example/${input:example}/"]
    }
  ],
  "inputs": [
    {
      "id": "example",
      "type": "promptString",
      "description": "what example do you want to run",
      "default": "cookie-redirect"
    }
  ]
}
